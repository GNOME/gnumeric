<?xml version="1.0"?>
<plugin id="Gnumeric_html">
	<information>
		<_name>HTML &amp; TeX</_name>
		<_description>Import/Export of HTML, TeX, DVI, roff</_description>
	</information>
	<loader type="g_module">
		<attribute name="module_file" value="html.la:dlname"/>
	</loader>
	<services>
		<service type="file_opener" id="html32" priority="100" probe="FALSE" open="TRUE" import="TRUE">
			<information>
				<_description>HTML file made by gnumeric</_description>
			</information>
			<file_patterns>
				<file_pattern type="shell_pattern" value="*.html"/>
				<file_pattern type="shell_pattern" value="*.htm"/>
			</file_patterns>
			<save_info saver_id="html32" format_level="auto"/>
		</service>
		<service type="file_saver" id="html32" file_extension="html" format_level="write_only">
			<information>
				<_description>HTML 3.2 (*.html)</_description>
			</information>
		</service>
		<service type="file_saver" id="html40" file_extension="html" format_level="write_only">
			<information>
				<_description>HTML 4.0 (*.html)</_description>
			</information>
		</service>
		<service type="file_saver" id="html40frag" file_extension="html" format_level="write_only">
			<information>
				<_description>HTML (*.html) fragment</_description>
			</information>
		</service>
		<service type="file_saver" id="latex" file_extension="tex" format_level="write_only">
			<information>
				<_description>LaTeX 2e (*.tex)</_description>
			</information>
		</service>
		<service type="file_saver" id="roff_dvi" file_extension="dvi" format_level="write_only">
			<information>
				<_description>DVI TeX (via groff)</_description>
			</information>
		</service>
		<service type="file_saver" id="roff" file_extension="me" format_level="write_only">
			<information>
				<_description>TROFF (*.me)</_description>
			</information>
		</service>
	</services>
</plugin>
