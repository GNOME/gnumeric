<?xml version="1.0" encoding="UTF-8"?>
<plugin id="Gnumeric_QPro">
	<information>
		<_name>Quatro Pro(tm)</_name>
		<_description>Imports Quatro Pro (tm) files</_description>
	</information>
	<loader type="Gnumeric_Builtin:module">
		<attribute name="module_file" value="qpro.la"/>
	</loader>
	<services>
		<service type="file_opener" id="qpro" priority="100" probe="TRUE">
			<information>
				<_description>Quatro Pro (*.wb1, *.wb2, *.wb3)</_description>
			</information>
			<suffixes>
				<suffix>wb1</suffix>
				<suffix>wb2</suffix>
				<suffix>wb3</suffix>
			</suffixes>
		</service>
	</services>
</plugin>
