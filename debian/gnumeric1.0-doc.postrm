#! /bin/sh
set -e

case "$1" in
	purge|remove)
		if [ -x /usr/bin/scrollkeeper-update ]; then
			/usr/bin/scrollkeeper-update
		fi
        ;;

	upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
        ;;

	*)
		echo "postrm called with unknown argument \`$1'" >&2
		exit 1
	;;
esac

#DEBHELPER#

exit 0
