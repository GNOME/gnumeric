		<_name>Component : Gnumeric</_name>
		<_description>Spreadsheets</_description>
	</information>
	<loader type="Gnumeric_Builtin:module">
		<attribute name="module_file" value="gnumeric"/>
	</loader>
	<services>
		<service type="component_engine" id="GOGnmComponent">
			<information>
				<_description>Spreadsheets component engine</_description>
			</information>
		</service>
