      <refsect1>
        <title>Description</title>
        <para>ISREF returns TRUE if the value is a reference.</para>
        <para>This function is Excel compatible.</para>
    </refsect1>      <refsect1>
        <title>Examples</title>
        <para></para>
        <para>ISREF(A1) equals TRUE.</para>        <para></para>
    </refsect1>
    <refsect1><title>See also</title>
      <para>      </para>
    </refsect1>
  </refentry>



  <refentry id="gnumeric-ISTEXT">
