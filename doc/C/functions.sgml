  <refentry id="gnumeric-RSQ">
    <refmeta>
      <refentrytitle>RSQ</refentrytitle>
    </refmeta>
    <refnamediv>
      <refname>RSQ</refname>
      <refpurpose></refpurpose>
    </refnamediv>
