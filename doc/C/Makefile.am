	autosave.sgml				\
	cell_refer.sgml				\
	copying-moving.sgml			\
	dndselection.sgml			\
	editing.sgml				\
	files.sgml				\
	filter.sgml				\
	formulas.sgml				\
	functions.sgml				\
	introduction.sgml			\
	license.sgml				\
