msgid ""
"@FUNCTION=LOWER\n"
"@SYNTAX=LOWER(text)\n"
"@DESCRIPTION=Returns a lower-case version of the string in @text\n"
"@SEEALSO=UPPER"
msgstr ""

#: src/fn-string.c:207
msgid ""
"@FUNCTION=MID\n"
"@SYNTAX=MID(string, position, length)\n"
"@DESCRIPTION=Returns a substring from @string starting at @position for "
"@lenght characters.\n"
"@SEEALSO=LEFT, RIGHT"
