#: src/functions/fn-database.c:491
msgid ""
"@FUNCTION=DAVERAGE\n"
"@SYNTAX=DAVERAGE(database,field,criteria)\n"
"@DESCRIPTION=DAVERAGE function returns the average of the values in a list "
"or database that match conditions specified. \n"
msgstr ""
"@FUNCTION=DAVERAGE\n"
"@SYNTAX=DAVERAGE(database,felt,kriterie)\n"
"@DESCRIPTION=DAVERAGE-funktionen returnerer gennemsnittet af værdierne i en "
"liste eller en database som passer til de specificerede betingelser. \n"

#: src/functions/fn-database.c:560
msgid ""
"@FUNCTION=DCOUNT\n"
"@SYNTAX=DCOUNT(database,field,criteria)\n"
"@DESCRIPTION=DCOUNT function counts the cells that contain numbers in a "
"database that match conditions specified. \n"
msgstr ""
"@FUNCTION=DCOUNT\n"
"@SYNTAX=DCOUNT(database,felt,kriterie)\n"
"@DESCRIPTION=DCOUNT-funktionen tæller cellene som indeholder tal i en "
"database som passer til de opgivede betingelser. \n"

#: src/functions/fn-database.c:621
msgid ""
"@FUNCTION=DCOUNTA\n"
"@SYNTAX=DCOUNTA(database,field,criteria)\n"
"@DESCRIPTION=DCOUNTA function counts the cells that contain data in a "
"database that match conditions specified. \n"
msgstr ""
"@FUNCTION=DCOUNTA\n"
"@SYNTAX=DCOUNTA(database,felt,kriterie)\n"
"@DESCRIPTION=DCOUNTA-funktionen tæller cellene som indeholder data i en "
"database som passer til de opgivede betingelser. \n"

#: src/functions/fn-database.c:679
msgid ""
"@FUNCTION=DGET\n"
"@SYNTAX=DGET(database,field,criteria)\n"
"@DESCRIPTION=DGET function returns a single value from a column that match "
"conditions specified. \n"
msgstr ""
"@FUNCTION=DGET\n"
"@SYNTAX=DGET(database,felt,kriterie)\n"
"@DESCRIPTION=DGET-funktionen returnerer en enkelt værdi fra en kolonne som "
"tilfredstiller de specificerede betingelser. \n"

#: src/functions/fn-database.c:749
msgid ""
"@FUNCTION=DMAX\n"
"@SYNTAX=DMAX(database,field,criteria)\n"
"@DESCRIPTION=DMAX function returns the largest number in a column that match "
"conditions specified. \n"
msgstr ""
"@FUNCTION=DMAX\n"
"@SYNTAX=DMAX(database,felt,kriterie)\n"
"@DESCRIPTION=DMAX-funktionen returnerer det største tal i en kolonne som "
"tilfredstiller de specificerede betingelser. \n"

#: src/functions/fn-database.c:820
msgid ""
"@FUNCTION=DMIN\n"
"@SYNTAX=DMIN(database,field,criteria)\n"
"@DESCRIPTION=DMIN function returns the smallest number in a column that "
"match conditions specified. \n"
msgstr ""
"@FUNCTION=DMIN\n"
"@SYNTAX=DMIN(database,felt,kriterie)\n"
"@DESCRIPTION=DMIN-funktionen returnerer det minste tal i en kolonne som "
"tilfredstiller de specificerede betingelser. \n"

#: src/functions/fn-database.c:890
msgid ""
"@FUNCTION=DPRODUCT\n"
"@SYNTAX=DPRODUCT(database,field,criteria)\n"
"@DESCRIPTION=DPRODUCT function returns the product of numbers in a column "
"that match conditions specified. \n"
msgstr ""
"@FUNCTION=DPRODUCT\n"
"@SYNTAX=DPRODUCT(database,felt,kriterie)\n"
"@DESCRIPTION=DPRODUCT-funktionen returnerer produktet af tallene i en "
"kolonne som tilfredstiller de specificerede betingelser. \n"

#: src/functions/fn-database.c:958
msgid ""
"@FUNCTION=DSTDEV\n"
"@SYNTAX=DSTDEV(database,field,criteria)\n"
"@DESCRIPTION=DSTDEV function returns the estimate of the standard deviation "
"of a population based on a sample. The populations consists of numbers that "
"match conditions specified. \n"
msgstr ""
"@FUNCTION=DSTDEV\n"
"@SYNTAX=DSTDEV(database,felt,kriterie)\n"
"@DESCRIPTION=DSTDEV-funktionen returnerer det estimerede standardafvigelse "
"for en population baseret på et udvalg. Populationen består af tal som til- "
"fredstiller de specificerede betingelser. \n"

#: src/functions/fn-database.c:1024
msgid ""
"@FUNCTION=DSTDEVP\n"
"@SYNTAX=DSTDEVP(database,field,criteria)\n"
"@DESCRIPTION=DSTDEVP function returns the standard deviation of a population "
"based on the entire populations. The populations consists of numbers that "
"match conditions specified. \n"
msgstr ""
"@FUNCTION=DSTDEVP\n"
"@SYNTAX=DSTDEVP(database,felt,kriterie)\n"
"@DESCRIPTION=DSTDEVP-funktionen returnerer standardafvigelsen for en "
"population baseret på hele populationen. Populationenen består af tal som "
"til- fredstiller de specificerede betingelser. \n"

#: src/functions/fn-database.c:1092
msgid ""
"@FUNCTION=DSUM\n"
"@SYNTAX=DSUM(database,field,criteria)\n"
"@DESCRIPTION=DSUM function returns the sum of numbers in a column that match "
"conditions specified. \n"
msgstr ""
"@FUNCTION=DSUM\n"
"@SYNTAX=DSUM(database,felt,kriterie)\n"
"@DESCRIPTION=DSUM-funktionen returnerer summen af tallene i en kolonne som "
"tilfredsstiller de specificerede betingelser. \n"

#: src/functions/fn-database.c:1159
msgid ""
"@FUNCTION=DVAR\n"
"@SYNTAX=DVAR(database,field,criteria)\n"
"@DESCRIPTION=DVAR function returns the estimate of variance of a population "
"based on a sample. The populations consists of numbers that match conditions "
"specified. \n"
msgstr ""
"@FUNCTION=DVAR\n"
"@SYNTAX=DVAR(database,felt,kriterie)\n"
"@DESCRIPTION=DVAR-funktionen returnerer estimeret varians for en population "
"baseret på et udvalg. Populationerne består af tal som tilfredsstiller de "
"specificerede betingelser. \n"

#: src/functions/fn-database.c:1227
msgid ""
"@FUNCTION=DVARP\n"
"@SYNTAX=DVARP(database,field,criteria)\n"
"@DESCRIPTION=DVARP function returns the variance of a population based on "
"the entire populations. The populations consists of numbers that match "
"conditions specified. \n"
msgstr ""
"@FUNCTION=DVARP\n"
"@SYNTAX=DVARP(database,felt,kriterie)\n"
"@DESCRIPTION=DVARP-funktionen returnerer variansen til en population baseret "
"på alle populationerne. Populationerne består af tal som tilfredsstiller de "
"specificerede betingelser. \n"

#: src/functions/fn-database.c:1297
msgid ""
"@FUNCTION=GETPIVOTDATA\n"
"@SYNTAX=GETPIVOTDATA(pivot_table,field_name)\n"
"@DESCRIPTION=GETPIVOTDATA function fetches summary data from a pivot table. "
"@pivot_table is a cell range containing the pivot table. @field_name is the "
"name of the field of which you want the summary data. \n"
"If the summary data is unavailable, GETPIVOTDATA returns #REF! error. \n"
"@SEEALSO="
msgstr ""
"@FUNCTION=GETPIVOTDATA\n"
"@SYNTAX=GETPIVOTDATA(pivot_tabel,felt_navn)\n"
"@DESCRIPTION=GETPIVOTDATA-funktionen henter sammendragsdata fra en pivottabel. "
"@pivot_tabel er et celleområde der indeholder pivot-tabellen. @felt_navn er "
"navnet på det felt som du ønsker sammendragsdataene fra. \n"
"Hvis sammendragsdataene ikke er tilgængelige, returnerer GETPIVOTDATA en #REF! fejl. \n"
"@SEEALSO="

