"@EXAMPLES=\n"
"EXACT(\"key\",\"key\") equals TRUE.\n"
"EXACT(\"key\",\"Key\") equals FALSE.\n"
"\n"
"@SEEALSO=LEN"
msgstr ""
"@FUNCTION=EXACT\n"
"@SYNTAX=EXACT(streng1, streng2)\n"
