#: plugins/fn-database/plugin.xml.in.h:1 plugins/gda/plugin.xml.in.h:1
msgid "Database"
msgstr "Database"

#: plugins/fn-database/plugin.xml.in.h:2
msgid "Database Functions"
msgstr "Funzioni per database"

