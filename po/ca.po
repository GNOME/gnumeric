#: src/functions/fn-eng.c:630
msgid ""
"@FUNCTION=COMPLEX\n"
"@SYNTAX=COMPLEX(real,im[,suffix])\n"
"@DESCRIPTION=COMPLEX returns a complex number of the form x + yi. @real is "
"the real and @im is the imaginary coefficient of the complex number.  "
"@suffix is the suffix for the imaginary coefficient.  If it is omitted, "
"COMPLEX uses 'i' by default.\n"
"If @suffix is neither 'i' nor 'j', COMPLEX returns #VALUE! error. This "
"function is Excel compatible. \n"
"@EXAMPLES=\n"
"COMPLEX(1,-1) equals 1-i.\n"
"\n"
"@SEEALSO="
msgstr ""
"@FUNCTION=COMPLEX\n"
"@SYNTAX=COMPLEX(real,im[,sufix])\n"
"@DESCRIPTION=COMPLEX retorna un nombre complex en la forma x + yi. @real és "
"el coeficient real i @im l'imaginari del nombre complex.  @sufix és el sufix "
"pel coeficient imaginari.  Si s'omet, COMPLEX empra 'i' per defecte.\n"
"Si @suffix no és 'i' ni 'j', COMPLEX retorna l'error #VALUE!. Aquesta funció "
"és compatible amb Excel. \n"
"@EXAMPLES=\n"
"COMPLEX(1,-1) és igual a 1-i.\n"
"\n"
"@SEEALSO="

#: src/functions/fn-eng.c:674
msgid ""
"@FUNCTION=IMAGINARY\n"
"@SYNTAX=IMAGINARY(inumber)\n"
"@DESCRIPTION=IMAGINARY returns the imaginary coefficient of a complex "
"number. This function is Excel compatible. \n"
"@EXAMPLES=\n"
"IMAGINARY(\"132-j\") equals -1.\n"
"\n"
"@SEEALSO=IMREAL"
msgstr ""
"@FUNCTION=IMAGINARY\n"
"@SYNTAX=IMAGINARY(nombrei)\n"
"@DESCRIPTION=IMAGINARY retorna el coeficient imaginari d'un nombre complex. "
"Aquesta funció és comptaible Excel. \n"
"@EXAMPLES=\n"
"IMAGINARY(\"132-j\") és igual a -1.\n"
"\n"
"@SEEALSO=IMREAL"

#: src/functions/fn-eng.c:705
msgid ""
"@FUNCTION=IMREAL\n"
"@SYNTAX=IMREAL(inumber)\n"
"@DESCRIPTION=IMREAL returns the real coefficient of a complex number. This "
"function is Excel compatible. \n"
"@EXAMPLES=\n"
"imreal(\"132-j\") equals 132.\n"
"\n"
"@SEEALSO=IMAGINARY"
msgstr ""
"@FUNCTION=IMREAL\n"
"@SYNTAX=IMREAL(nombrei)\n"
"@DESCRIPTION=IMREAL retorna el coeficient real d'un nombre complex. Aquesta "
"funció és compatible amb Excel. \n"
"@EXAMPLES=\n"
"imreal(\"132-j\") és igual a 132.\n"
"\n"
"@SEEALSO=IMAGINARY"

#: src/functions/fn-eng.c:736
msgid ""
"@FUNCTION=IMABS\n"
"@SYNTAX=IMABS(inumber)\n"
"@DESCRIPTION=IMABS returns the absolute value of a complex number. This "
"function is Excel compatible. \n"
"@EXAMPLES=\n"
"IMABS(\"2-j\") equals 2.23606798.\n"
"\n"
"@SEEALSO=IMAGINARY,IMREAL"
msgstr ""
"@FUNCTION=IMABS\n"
"@SYNTAX=IMABS(nombrei)\n"
"@DESCRIPTION=IMABS retorna el valor absolut d'un nombre complex. Aquesta "
"funció és compatible Excel. \n"
"@EXAMPLES=\n"
"IMABS(\"2-j\") és igual a 2.23606798.\n"
"\n"
"@SEEALSO=IMAGINARY,IMREAL"

#: src/functions/fn-eng.c:767
msgid ""
"@FUNCTION=IMCONJUGATE\n"
"@SYNTAX=IMCONJUGATE(inumber)\n"
"@DESCRIPTION=IMCONJUGATE returns the complex conjugate of a complex number. "
"This function is Excel compatible. \n"
"@EXAMPLES=\n"
"IMCONJUGATE(\"1-j\") equals 1+j.\n"
"\n"
"@SEEALSO=IMAGINARY,IMREAL"
msgstr ""
"@FUNCTION=IMCONJUGATE\n"
"@SYNTAX=IMCONJUGATE(nombrei)\n"
"@DESCRIPTION=IMCONJUGATE retorna el complex conjugat d'un nombre complex. "
"Aquesta funció és compatible amb Excel. \n"
"@EXAMPLES=\n"
"IMCONJUGATE(\"1-j\") és igual a 1+j.\n"
"\n"
"@SEEALSO=IMAGINARY,IMREAL"

#: src/functions/fn-eng.c:798
msgid ""
"@FUNCTION=IMCOS\n"
"@SYNTAX=IMCOS(inumber)\n"
"@DESCRIPTION=IMCOS returns the cosine of a complex number. This function is "
"Excel compatible. \n"
"@EXAMPLES=\n"
"IMCOS(\"1+j\") equals 0.833730-0.988898j.\n"
"\n"
"@SEEALSO=IMSIN,IMTAN"
msgstr ""
