#: src/functions/fn-database.c:491
msgid ""
"@FUNCTION=DAVERAGE\n"
"@SYNTAX=DAVERAGE(database,field,criteria)\n"
"@DESCRIPTION=DAVERAGE function returns the average of the values in a list "
"or database that match conditions specified. \n"
msgstr ""
"@FUNCTION=DAVERAGE\n"
"@SYNTAX=DAVERAGE(database,felt,kriterie)\n"
"@DESCRIPTION=DAVERAGE-funksjonen returnerer gjennomsnittet av verdiene i en "
"liste eller en database som passer til de spesifiserte betingelsene. \n"

#: src/functions/fn-database.c:560
msgid ""
"@FUNCTION=DCOUNT\n"
"@SYNTAX=DCOUNT(database,field,criteria)\n"
"@DESCRIPTION=DCOUNT function counts the cells that contain numbers in a "
"database that match conditions specified. \n"
msgstr ""
"@FUNCTION=DCOUNT\n"
"@SYNTAX=DCOUNT(database,felt,kriterie)\n"
"@DESCRIPTION=DCOUNT-funksjonen teller cellene som inneholder tall i en "
"database som passer de oppgitte betingelsene. \n"

#: src/functions/fn-database.c:621
msgid ""
"@FUNCTION=DCOUNTA\n"
"@SYNTAX=DCOUNTA(database,field,criteria)\n"
"@DESCRIPTION=DCOUNTA function counts the cells that contain data in a "
"database that match conditions specified. \n"
msgstr ""
"@FUNCTION=DCOUNTA\n"
"@SYNTAX=DCOUNTA(database,felt,kriterie)\n"
"@DESCRIPTION=DCOUNTA-funksjonen teller cellene som inneholder data i en "
"database som passer de oppgitte betingelsene. \n"

#: src/functions/fn-database.c:679
msgid ""
"@FUNCTION=DGET\n"
"@SYNTAX=DGET(database,field,criteria)\n"
"@DESCRIPTION=DGET function returns a single value from a column that match "
"conditions specified. \n"
msgstr ""
"@FUNCTION=DGET\n"
"@SYNTAX=DGET(database,felt,kriterie)\n"
"@DESCRIPTION=DGET-funksjonen returnerer en enkelt verdi fra en kolonne som "
"tilfredstiller de spesifiserte betingelsene. \n"

#: src/functions/fn-database.c:747
msgid ""
"@FUNCTION=DMAX\n"
"@SYNTAX=DMAX(database,field,criteria)\n"
"@DESCRIPTION=DMAX function returns the largest number in a column that match "
"conditions specified. \n"
msgstr ""
"@FUNCTION=DMAX\n"
"@SYNTAX=DMAX(database,felt,kriterie)\n"
"@DESCRIPTION=DMAX-funksjonen returnerer det største tallet i en kolonne som "
"tilfredstiller de spesifiserte betingelsene. \n"

#: src/functions/fn-database.c:818
msgid ""
"@FUNCTION=DMIN\n"
"@SYNTAX=DMIN(database,field,criteria)\n"
"@DESCRIPTION=DMIN function returns the smallest number in a column that "
"match conditions specified. \n"
msgstr ""
"@FUNCTION=DMIN\n"
"@SYNTAX=DMIN(database,felt,kriterie)\n"
"@DESCRIPTION=DMIN-funksjonen returnerer det minste tallet i en kolonne som "
"tilfredstiller de spesifiserte betingelsene. \n"

#: src/functions/fn-database.c:888
msgid ""
"@FUNCTION=DPRODUCT\n"
"@SYNTAX=DPRODUCT(database,field,criteria)\n"
"@DESCRIPTION=DPRODUCT function returns the product of numbers in a column "
"that match conditions specified. \n"
msgstr ""
"@FUNCTION=DPRODUCT\n"
"@SYNTAX=DPRODUCT(database,felt,kriterie)\n"
"@DESCRIPTION=DPRODUCT-funksjonen returnerer produktet av tallene i en "
"kolonne som tilfredstiller de spesifiserte betingelsene. \n"

#: src/functions/fn-database.c:955
msgid ""
"@FUNCTION=DSTDEV\n"
"@SYNTAX=DSTDEV(database,field,criteria)\n"
"@DESCRIPTION=DSTDEV function returns the estimate of the standard deviation "
"of a population based on a sample. The populations consists of numbers that "
"match conditions specified. \n"
msgstr ""
"@FUNCTION=DSTDEV\n"
"@SYNTAX=DSTDEV(database,felt,kriterie)\n"
"@DESCRIPTION=DSTDEV-funksjonen returnerer det estimerte standardavviket for "
"en populasjon basert på et utvalg. Populasjonen består av tall som til- "
"fredstiller de spesifiserte betingelsene. \n"

#: src/functions/fn-database.c:1021
msgid ""
"@FUNCTION=DSTDEVP\n"
"@SYNTAX=DSTDEVP(database,field,criteria)\n"
"@DESCRIPTION=DSTDEVP function returns the standard deviation of a population "
"based on the entire populations. The populations consists of numbers that "
"match conditions specified. \n"
msgstr ""
"@FUNCTION=DSTDEVP\n"
"@SYNTAX=DSTDEVP(database,felt,kriterie)\n"
"@DESCRIPTION=DSTDEVP-funksjonen returnerer standardavviket for en populasjon "
"basert på hele populasjonen. Populasjonenen består av tall som til- "
"fredstiller de spesifiserte betingelsene. \n"

#: src/functions/fn-database.c:1089
msgid ""
"@FUNCTION=DSUM\n"
"@SYNTAX=DSUM(database,field,criteria)\n"
"@DESCRIPTION=DSUM function returns the sum of numbers in a column that match "
"conditions specified. \n"
msgstr ""
"@FUNCTION=DSUM\n"
"@SYNTAX=DSUM(database,felt,kriterie)\n"
"@DESCRIPTION=DSUM-funksjonen returnerer summen av tallene i en kolonne som "
"møter de spesifiserte betingelsene. \n"

#: src/functions/fn-database.c:1156
msgid ""
"@FUNCTION=DVAR\n"
"@SYNTAX=DVAR(database,field,criteria)\n"
"@DESCRIPTION=DVAR function returns the estimate of variance of a population "
"based on a sample. The populations consists of numbers that match conditions "
"specified. \n"
msgstr ""
"@FUNCTION=DVAR\n"
"@SYNTAX=DVAR(database,felt,kriterie)\n"
"@DESCRIPTION=DVAR-funksjonen returnerer estimert varians for en populasjon "
"basert på et utvalg. Populasjonene består av tall som møter de spesifiserte "
"betingelsene. \n"

#: src/functions/fn-database.c:1224
msgid ""
"@FUNCTION=DVARP\n"
"@SYNTAX=DVARP(database,field,criteria)\n"
"@DESCRIPTION=DVARP function returns the variance of a population based on "
"the entire populations. The populations consists of numbers that match "
"conditions specified. \n"
msgstr ""
"@FUNCTION=DVARP\n"
"@SYNTAX=DVARP(database,felt,kriterie)\n"
"@DESCRIPTION=DVARP-funksjonen returnerer variansen til en populasjon basert "
"på hele populasjonene. Populasjonene består av tall som møter de "
"spesifiserte betingelsene. \n"

#: src/functions/fn-database.c:1293
msgid ""
"@FUNCTION=GETPIVOTDATA\n"
"@SYNTAX=GETPIVOTDATA(pivot_table,field_name)\n"
"@DESCRIPTION=GETPIVOTDATA function fetches summary data from a pivot table. "
"@pivot_table is a cell range containing the pivot table. @field_name is the "
"name of the field of which you want the summary data. \n"
"If the summary data is unavailable, GETPIVOTDATA returns #REF! error. \n"
"@SEEALSO="
msgstr ""

