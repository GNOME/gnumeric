"DCOUNT(A1:C7, \"Name\", A9:B11) equals 0.\n"
"\n"
"@SEEALSO=DAVERAGE"
msgstr ""
"\n"
"@EXAMPLES=\n"
"DCOUNT(A1:C7, \"Lønn\", A9:A11) er lik 3.\n"
"DCOUNT(A1:C7, \"Lønn\", A9:B11) er lik 2.\n"
"DCOUNT(A1:C7, \"Navn\", A9:B11) er lik 0.\n"
"\n"
"@SEEALSO=DAVERAGE"

