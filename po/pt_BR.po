# src/fn-misc.c:18
#: src/functions/fn-string.c:414
msgid ""
"@FUNCTION=CLEAN\n"
"@SYNTAX=CLEAN(string)\n"
"@DESCRIPTION=CLEAN removes any non-printable characters from @string.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"CLEAN(\"one\"\\&char(7)) equals \"one\".\n"
"\n"
"@SEEALSO="
msgstr ""
"@FUNCTION=CLEAN\n"
"@SYNTAX=CLEAN(cadeia)\n"
"@DESCRIPTION=CLEAN limpa a cadeia de qualquer caracteres não-imprimíveis.\n"
"Esta função é compatível com o Excel.\n"
"@EXAMPLES=\n"
"CLEAN(\"one\"\\&char(7)) retorna \"one\".\n"
"\n"
"@SEEALSO="

#: src/functions/fn-string.c:446
msgid ""
"@FUNCTION=FIND\n"
"@SYNTAX=FIND(string1,string2[,start])\n"
"@DESCRIPTION=FIND returns position of @string1 in @string2 (case-sensitive), "
"searching only from character @start onwards (assuming 1 if omitted).\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"FIND(\"ac\",\"Jack\") equals 2.\n"
"\n"
"@SEEALSO=EXACT, LEN, MID, SEARCH"
msgstr ""
"@FUNCTION=FIND\n"
"@SYNTAX=FIND(cadeia1,cadeia2[,cadeia])\n"
"@DESCRIPTION=FIND retorna primeira ocorrência da @cadeia1 na @cadeia2 "
"(sensível a maiúsculas), procurando a partir do caractere @início (assumido "
"1 se omitido).\n"
"Esta função é compatível com o Excel.\n"
"@EXAMPLES=\n"
"FIND(\"ac\",\"Jack\") retorna 2.\n"
"\n"
"@SEEALSO=EXACT, LEN, MID, SEARCH"

#: src/functions/fn-string.c:488
msgid ""
"@FUNCTION=FIXED\n"
"@SYNTAX=FIXED(num,[decimals, no_commas])\n"
"@DESCRIPTION=FIXED returns @num as a formatted string with @decimals numbers "
"after the decimal point, omitting commas if requested by @no_commas.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"FIXED(1234.567,2) equals \"1,234.57\".\n"
"\n"
"@SEEALSO="
msgstr ""
"@FUNCTION=FIXED\n"
"@SYNTAX=FIXED(num,[decimais, sem_vírgulas])\n"
"@DESTION=FIXED\n"
"@SYNTAX=FIXED(num,[decimais, sem_vírgulas])\n"
"@DESCRIPTION=FIXED retorna @num como uma cadeia formatada com @decimais "
"algarismos após o ponto decimal, omitindo vírgulas se requisitado por "
"@sem_vírgulas.\n"
"@EXAMPLES=\n"
"FIXED(1234.567,2) retorna \"1,234.57\".\n"
"\n"
"@SEEALSO="

#: src/functions/fn-string.c:556
msgid ""
"@FUNCTION=PROPER\n"
"@SYNTAX=PROPER(string)\n"
"@DESCRIPTION=PROPER returns @string with initial of each word capitalised.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"PROPER(\"j. f. kennedy\") equals \"J. F. Kennedy\".\n"
"\n"
"@SEEALSO=LOWER, UPPER"
msgstr ""
"@FUNCTION=PROPER\n"
"@SYNTAX=PROPER(cadeia)\n"
"@DESCRIPTION=PROPER retorna @cadeia com a inicial de cada palavra em "
"maiúscula.\n"
"Esta função é compátível com o Excel.\n"
"@EXAMPLES=\n"
"PROPER(\"j. f. kennedy\") retorna \"J. F. Kennedy\".\n"
"\n"
"@SEEALSO=LOWER, UPPER"

#: src/functions/fn-string.c:602
msgid ""
"@FUNCTION=REPLACE\n"
"@SYNTAX=REPLACE(old,start,num,new)\n"
"@DESCRIPTION=REPLACE returns @old with @new replacing @num characters from "
"@start.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"REPLACE(\"testing\",2,3,\"*****\") equals \"t*****ing\".\n"
"\n"
"@SEEALSO=MID, SEARCH, SUBSTITUTE, TRIM"
msgstr ""
"@FUNCTION=REPLACE\n"
"@SYNTAX=REPLACE(antigo,início,num,novo)\n"
"@DESCRIPTION=REPLACE retorna @antigo com @novo substituindo @num caracteres "
"a partir de @início.\n"
"Esta função é compátível com o Excel.\n"
"@EXAMPLES=\n"
"REPLACE(\"testando\",2,3,\"*****\") retorna \"t*****ando\".\n"
"\n"
"@SEEALSO=MID, SEARCH, SUBSTITUTE, TRIM"

#: src/functions/fn-string.c:655
msgid ""
"@FUNCTION=T\n"
"@SYNTAX=T(value)\n"
"@DESCRIPTION=T returns @value if and only if it is text, otherwise a blank "
"string.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"T(\"text\") equals \"text\".\n"
"T(64) returns an empty cell.\n"
"\n"
"@SEEALSO=CELL, N, VALUE"
msgstr ""
"@FUNCTION=T\n"
"@SYNTAX=T(valor)\n"
"@DESCRIPTION=T retorna o @valor se e somente se for texto, do contrário uma "
"cadeia em branco.\n"
"@EXAMPLES=\n"
"T(\"blah\") retorna \"blah\".\n"
"T(64) retorna uma célula vazia.\n"
"\n"
"@SEEALSO=CELL, N, VALUE"

#: src/functions/fn-string.c:681
msgid ""
"@FUNCTION=TEXT\n"
"@SYNTAX=TEXT(value,format_text)\n"
"@DESCRIPTION=TEXT returns @value as a string with the specified format.\n"
"This function is Excel compatible.\n"
"@EXAMPLES=\n"
"TEXT(3.223,\"$0.00\") equals \"$3.22\".\n"
"TEXT(date(1999,4,15),\"mmmm, dd, yy\") equals \"April, 15, 99\".\n"
"\n"
"@SEEALSO=DOLLAR"
msgstr ""
"@FUNCTION=TEXT\n"
"@SYNTAX=TEXT(valor,formato_texto)\n"
"@DESCRIPTION=TEXT retorna @valor como uma cadeia com o formato "
"especificado.\n"
"Esta função é compátível com o Excel.\n"
"@EXAMPLES=\n"
"TEXT(3.223,\"$0.00\") retorna \"$3.22\".\n"
"TEXT(date(1999,4,15),\"dd mmmm yy\") retorna \"15 Abril 99\".\n"
"\n"
"@SEEALSO=DOLLAR"

# src/expr.c:509 src/expr.c:514 src/expr.c:793 src/expr.c:964
