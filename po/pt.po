#: src/fn-misc.c:160
msgid ""
"@FUNCTION=NA\n"
"@SYNTAX=NA()\n"
"@DESCRIPTION=NA Returns the error value #N/A. \n"
"@SEEALSO="
msgstr ""

#: src/fn-misc.c:176
msgid ""
"@FUNCTION=ERROR\n"
"@SYNTAX=ERROR(text)\n"
"@DESCRIPTION=Return the specified error\n"
"\n"
"@SEEALSO=ISERROR"
