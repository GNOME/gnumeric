#: src/functions/fn-database.c:505
msgid ""
"@FUNCTION=DAVERAGE\n"
"@SYNTAX=DAVERAGE(database,field,criteria)\n"
"@DESCRIPTION=DAVERAGE function returns the average of the values in a list "
"or database that match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DAVERAGE\n"
"@ΣΥΝΤΑΞΗ=DAVERAGE(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DAVERAGE επιστρέφει το μέσο όρο των τιμών ενός "
"καταλόγου ή βάσης δεδομένων, η οποίες πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:513
msgid ""
"\n"
"@EXAMPLES=\n"
"DAVERAGE(A1:C7, \"Salary\", A9:A11) equals 42296.3333.\n"
"DAVERAGE(A1:C7, \"Age\", A9:A11) equals 39.\n"
"DAVERAGE(A1:C7, \"Salary\", A9:B11) equals 40782.5.\n"
"DAVERAGE(A1:C7, \"Age\", A9:B11) equals 36.\n"
"\n"
"@SEEALSO=DCOUNT"
msgstr ""

#: src/functions/fn-database.c:575
msgid ""
"@FUNCTION=DCOUNT\n"
"@SYNTAX=DCOUNT(database,field,criteria)\n"
"@DESCRIPTION=DCOUNT function counts the cells that contain numbers in a "
"database that match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DCOUNT\n"
"@ΣΥΝΤΑΞΗ=DCOUNT(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DCOUNT επιστρέφει το πλήθος των κελιών που περιέχουν "
"αριθμούς σε μία βάση δεδομένων και πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:583
msgid ""
"\n"
"@EXAMPLES=\n"
"DCOUNT(A1:C7, \"Salary\", A9:A11) equals 3.\n"
"DCOUNT(A1:C7, \"Salary\", A9:B11) equals 2.\n"
"DCOUNT(A1:C7, \"Name\", A9:B11) equals 0.\n"
"\n"
"@SEEALSO=DAVERAGE"
msgstr ""

#: src/functions/fn-database.c:637
msgid ""
"@FUNCTION=DCOUNTA\n"
"@SYNTAX=DCOUNTA(database,field,criteria)\n"
"@DESCRIPTION=DCOUNTA function counts the cells that contain data in a "
"database that match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DCOUNTA\n"
"@ΣΥΝΤΑΞΗ=DCOUNTA(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DCOUNTA επιστρέφει το πλήθος των κελιών που περιέχουν "
"δεδομένα σε μία βάση δεδομένων και πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:645
msgid ""
"\n"
"@EXAMPLES=\n"
"DCOUNTA(A1:C7, \"Salary\", A9:A11) equals 3.\n"
"DCOUNTA(A1:C7, \"Salary\", A9:B11) equals 2.\n"
"DCOUNTA(A1:C7, \"Name\", A9:B11) equals 2.\n"
"\n"
"@SEEALSO=DCOUNT"
msgstr ""

#: src/functions/fn-database.c:696
msgid ""
"@FUNCTION=DGET\n"
"@SYNTAX=DGET(database,field,criteria)\n"
"@DESCRIPTION=DGET function returns a single value from a column that match "
"conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DGET\n"
"@ΣΥΝΤΑΞΗ=DGET(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DGET επιστρέφει μία μόνο τιμή από μια στήλη που "
"πληρεί τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:704
msgid ""
"\n"
"If none of the items match the conditions, DGET returns #VALUE! error. If "
"more than one items match the conditions, DGET returns #NUM! error. \n"
"@EXAMPLES=\n"
"DGET(A1:C7, \"Salary\", A9:A10) equals 34323.\n"
"DGET(A1:C7, \"Name\", A9:A10) equals \"Clark\".\n"
"\n"
"@SEEALSO=DCOUNT"
msgstr ""

#: src/functions/fn-database.c:765
msgid ""
"@FUNCTION=DMAX\n"
"@SYNTAX=DMAX(database,field,criteria)\n"
"@DESCRIPTION=DMAX function returns the largest number in a column that match "
"conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DMAX\n"
"@ΣΥΝΤΑΞΗ=DMAX(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DMAX επιστρέφει το μεγαλύτερο αριθμό από μια στήλη "
"που πληρεί τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:773
msgid ""
"\n"
"@EXAMPLES=\n"
"DMAX(A1:C7, \"Salary\", A9:A11) equals 47242.\n"
"DMAX(A1:C7, \"Age\", A9:A11) equals 45.\n"
"DMAX(A1:C7, \"Age\", A9:B11) equals 43.\n"
"\n"
"@SEEALSO=DMIN"
msgstr ""

#: src/functions/fn-database.c:837
msgid ""
"@FUNCTION=DMIN\n"
"@SYNTAX=DMIN(database,field,criteria)\n"
"@DESCRIPTION=DMIN function returns the smallest number in a column that "
"match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DMIN\n"
"@ΣΥΝΤΑΞΗ=DMIN(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DMIN επιστρέφει το μικρότερο αριθμό από μια στήλη που "
"πληρεί τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:845
msgid ""
"\n"
"@EXAMPLES=\n"
"DMIN(A1:C7, \"Salary\", A9:B11) equals 34323.\n"
"DMIN(A1:C7, \"Age\", A9:B11) equals 29.\n"
"\n"
"@SEEALSO=DMAX"
msgstr ""

#: src/functions/fn-database.c:908
msgid ""
"@FUNCTION=DPRODUCT\n"
"@SYNTAX=DPRODUCT(database,field,criteria)\n"
"@DESCRIPTION=DPRODUCT function returns the product of numbers in a column "
"that match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DPRODUCT\n"
"@ΣΥΝΤΑΞΗ=DPRODUCT(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DPRODUCT επιστρέφει το γινόμενο των αριθμών από μία "
"στήλη που πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:916
msgid ""
"\n"
"@EXAMPLES=\n"
"DPRODUCT(A1:C7, \"Age\", A9:B11) equals 1247.\n"
"\n"
"@SEEALSO=DSUM"
msgstr ""

#: src/functions/fn-database.c:976
msgid ""
"@FUNCTION=DSTDEV\n"
"@SYNTAX=DSTDEV(database,field,criteria)\n"
"@DESCRIPTION=DSTDEV function returns the estimate of the standard deviation "
"of a population based on a sample. The populations consists of numbers that "
"match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DSTDEV\n"
"@ΣΥΝΤΑΞΗ=DSTDEV(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@DESCRIPTION=Η συνάρτηση DSTDEV επιστρέφει την εκτίμηση της τυπικής "
"απόκλισης ενός πληθυσμού βάσει ενός δείγματος. Ο πληθυσμός αποτελείται από "
"τους αριθμούς που πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:985
msgid ""
"\n"
"@EXAMPLES=\n"
"DSTDEV(A1:C7, \"Age\", A9:B11) equals 9.89949.\n"
"DSTDEV(A1:C7, \"Salary\", A9:B11) equals 9135.112506.\n"
"\n"
"@SEEALSO=DSTDEVP"
msgstr ""

#: src/functions/fn-database.c:1043
msgid ""
"@FUNCTION=DSTDEVP\n"
"@SYNTAX=DSTDEVP(database,field,criteria)\n"
"@DESCRIPTION=DSTDEVP function returns the standard deviation of a population "
"based on the entire populations. The populations consists of numbers that "
"match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DSTDEVP\n"
"@ΣΥΝΤΑΞΗ=DSTDEVP(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@DESCRIPTION=Η συνάρτηση DSTDEVP επιστρέφει την τυπική απόκλιση ενός "
"πληθυσμού βάσει όλου του πληθυσμού. Ο πληθυσμός αποτελείται από τους "
"αριθμούς που πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:1052
msgid ""
"\n"
"@EXAMPLES=\n"
"DSTDEVP(A1:C7, \"Age\", A9:B11) equals 7.\n"
"DSTDEVP(A1:C7, \"Salary\", A9:B11) equals 6459.5.\n"
"\n"
"@SEEALSO=DSTDEV"
msgstr ""

#: src/functions/fn-database.c:1112
msgid ""
"@FUNCTION=DSUM\n"
"@SYNTAX=DSUM(database,field,criteria)\n"
"@DESCRIPTION=DSUM function returns the sum of numbers in a column that match "
"conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DSUM\n"
"@ΣΥΝΤΑΞΗ=DSUM(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DSUM επιστρέφει το άθροισμα των αριθμών από μία στήλη "
"που πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:1120
msgid ""
"\n"
"@EXAMPLES=\n"
"DSUM(A1:C7, \"Age\", A9:B11) equals 72.\n"
"DSUM(A1:C7, \"Salary\", A9:B11) equals 81565.\n"
"\n"
"@SEEALSO=DPRODUCT"
msgstr ""

#: src/functions/fn-database.c:1180
msgid ""
"@FUNCTION=DVAR\n"
"@SYNTAX=DVAR(database,field,criteria)\n"
"@DESCRIPTION=DVAR function returns the estimate of variance of a population "
"based on a sample. The populations consists of numbers that match conditions "
"specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DVAR\n"
"@ΣΥΝΤΑΞΗ=DVAR(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@DESCRIPTION=Η συνάρτηση DVAR επιστρέφει την εκτίμηση της διακύμανσης ενός "
"πληθυσμού βάσει ενός δείγματος. Ο πληθυσμός αποτελείται από τους αριθμούς "
