#: src/functions/fn-database.c:491
msgid ""
"@FUNCTION=DAVERAGE\n"
"@SYNTAX=DAVERAGE(database,field,criteria)\n"
"@DESCRIPTION=DAVERAGE function returns the average of the values in a list "
"or database that match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DAVERAGE\n"
"@ΣΥΝΤΑΞΗ=DAVERAGE(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DAVERAGE επιστρέφει το μέσο όρο των τιμών ενός καταλόγου "
"ή βάσης δεδομένων, η οποίες πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:560
msgid ""
"@FUNCTION=DCOUNT\n"
"@SYNTAX=DCOUNT(database,field,criteria)\n"
"@DESCRIPTION=DCOUNT function counts the cells that contain numbers in a "
"database that match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DCOUNT\n"
"@ΣΥΝΤΑΞΗ=DCOUNT(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DCOUNT επιστρέφει το πλήθος των κελιών που περιέχουν "
"αριθμούς σε μία βάση δεδομένων και πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:621
msgid ""
"@FUNCTION=DCOUNTA\n"
"@SYNTAX=DCOUNTA(database,field,criteria)\n"
"@DESCRIPTION=DCOUNTA function counts the cells that contain data in a "
"database that match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DCOUNTA\n"
"@ΣΥΝΤΑΞΗ=DCOUNTA(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DCOUNTA επιστρέφει το πλήθος των κελιών που περιέχουν "
"δεδομένα σε μία βάση δεδομένων και πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:679
msgid ""
"@FUNCTION=DGET\n"
"@SYNTAX=DGET(database,field,criteria)\n"
"@DESCRIPTION=DGET function returns a single value from a column that match "
"conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DGET\n"
"@ΣΥΝΤΑΞΗ=DGET(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DGET επιστρέφει μία μόνο τιμή από μια στήλη που πληρεί "
"τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:749
msgid ""
"@FUNCTION=DMAX\n"
"@SYNTAX=DMAX(database,field,criteria)\n"
"@DESCRIPTION=DMAX function returns the largest number in a column that match "
"conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DMAX\n"
"@ΣΥΝΤΑΞΗ=DMAX(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DMAX επιστρέφει το μεγαλύτερο αριθμό από μια στήλη που "
"πληρεί τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:820
msgid ""
"@FUNCTION=DMIN\n"
"@SYNTAX=DMIN(database,field,criteria)\n"
"@DESCRIPTION=DMIN function returns the smallest number in a column that "
"match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DMIN\n"
"@ΣΥΝΤΑΞΗ=DMIN(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DMIN επιστρέφει το μικρότερο αριθμό από μια στήλη που "
"πληρεί τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:890
msgid ""
"@FUNCTION=DPRODUCT\n"
"@SYNTAX=DPRODUCT(database,field,criteria)\n"
"@DESCRIPTION=DPRODUCT function returns the product of numbers in a column "
"that match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DPRODUCT\n"
"@ΣΥΝΤΑΞΗ=DPRODUCT(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DPRODUCT επιστρέφει το γινόμενο των αριθμών από μία "
"στήλη που πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:958
msgid ""
"@FUNCTION=DSTDEV\n"
"@SYNTAX=DSTDEV(database,field,criteria)\n"
"@DESCRIPTION=DSTDEV function returns the estimate of the standard deviation "
"of a population based on a sample. The populations consists of numbers that "
"match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DSTDEV\n"
"@ΣΥΝΤΑΞΗ=DSTDEV(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@DESCRIPTION=Η συνάρτηση DSTDEV επιστρέφει την εκτίμηση της τυπικής απόκλισης "
"ενός πληθυσμού βάσει ενός δείγματος. Ο πληθυσμός αποτελείται από τους αριθμούς "
"που πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:1024
msgid ""
"@FUNCTION=DSTDEVP\n"
"@SYNTAX=DSTDEVP(database,field,criteria)\n"
"@DESCRIPTION=DSTDEVP function returns the standard deviation of a population "
"based on the entire populations. The populations consists of numbers that "
"match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DSTDEVP\n"
"@ΣΥΝΤΑΞΗ=DSTDEVP(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@DESCRIPTION=Η συνάρτηση DSTDEVP επιστρέφει την τυπική απόκλιση ενός "
"πληθυσμού βάσει όλου του πληθυσμού. Ο πληθυσμός αποτελείται από τους αριθμούς "
"που πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:1092
msgid ""
"@FUNCTION=DSUM\n"
"@SYNTAX=DSUM(database,field,criteria)\n"
"@DESCRIPTION=DSUM function returns the sum of numbers in a column that match "
"conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DSUM\n"
"@ΣΥΝΤΑΞΗ=DSUM(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DSUM επιστρέφει το άθροισμα των αριθμών από μία "
"στήλη που πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:1159
msgid ""
"@FUNCTION=DVAR\n"
"@SYNTAX=DVAR(database,field,criteria)\n"
"@DESCRIPTION=DVAR function returns the estimate of variance of a population "
"based on a sample. The populations consists of numbers that match conditions "
"specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DVAR\n"
"@ΣΥΝΤΑΞΗ=DVAR(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@DESCRIPTION=Η συνάρτηση DVAR επιστρέφει την εκτίμηση της διακύμανσης "
"ενός πληθυσμού βάσει ενός δείγματος. Ο πληθυσμός αποτελείται από τους αριθμούς "
"που πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:1227
msgid ""
"@FUNCTION=DVARP\n"
"@SYNTAX=DVARP(database,field,criteria)\n"
"@DESCRIPTION=DVARP function returns the variance of a population based on "
"the entire populations. The populations consists of numbers that match "
"conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DVARP\n"
"@ΣΥΝΤΑΞΗ=DVARP(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@DESCRIPTION=Η συνάρτηση DVARP επιστρέφει την διακύμανση ενός "
"πληθυσμού βάσει όλου του πληθυσμού. Ο πληθυσμός αποτελείται από τους αριθμούς "
"που πληρούν τις καθορισμένες συνθήκες. \n"

# FIX todo

#: src/functions/fn-database.c:1297
msgid ""
"@FUNCTION=GETPIVOTDATA\n"
"@SYNTAX=GETPIVOTDATA(pivot_table,field_name)\n"
"@DESCRIPTION=GETPIVOTDATA function fetches summary data from a pivot table. "
"@pivot_table is a cell range containing the pivot table. @field_name is the "
"name of the field of which you want the summary data. \n"
"If the summary data is unavailable, GETPIVOTDATA returns #REF! error. \n"
"@SEEALSO="
msgstr ""

#: src/functions/fn-database.c:1339
msgid "Database"
msgstr "Βάση δεδομένων"

#: src/functions/fn-date.c:22
msgid ""
"@FUNCTION=DATE\n"
"@SYNTAX=DATE (year,month,day)\n"
"@DESCRIPTION=Computes the number of days since the 1st of january of "
"1900(the date serial number) for the given year, month and day.\n"
"The @day might be negative (to count backwards) and it is relative to the "
"previous @month.  The @years should be at least 1900.\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO=TODAY, NOW"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DATE\n"
"@ΣΥΝΤΑΞΗ=DATE (έτος,μήνας,ημέρα)\n"
"@ΠΕΡΙΓΡΑΦΗ=Υπολογίζει τον αριθμό των ημερών από την 1η Ιανουαρίου του "
"1900 (σειριακός αριθμός ημ/νίας) για το δεδομένο έτος, μήνα και ημέρα.\n"
"Η @ημέρα μπορεί να είναι αρνητική (για ανάποδο μέτρημα) και είναι σχετική "
"με τον προηγούμενο @μήνα.  Το @έτος πρέπει να είναι τουλάχιστον 1900.\n"
"@ΠΑΡΑΔΕΙΓΜΑΤΑ=\n"
"\n"
"@ΔΕΙΤΕ=TODAY, NOW"

#: src/functions/fn-date.c:55
msgid "Invalid month or year"
msgstr "Άκυρος μήνας ή έτος"

#: src/functions/fn-date.c:67
msgid "Invalid day"
msgstr "Άκυρη ημέρα"

#: src/functions/fn-date.c:77
msgid ""
"@FUNCTION=DATEVALUE\n"
"@SYNTAX=DATEVALUE(date_str)\n"
"@DESCRIPTION=DATEVALUE returns the serial number of the date.  @date_str is "
"the string that contains the date.  For example, DATEVALUE(\"1/1/1999\") "
"equals to 36160. \n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO=DATE"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DATEVALUE\n"
"@ΣΥΝΤΑΞΗ=DATEVALUE(αλφαρ_ημερομ)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η DATEVALUE επιστρέφει το σειριακό αριθμό της ημ/νίας.  Το @αλφαρ_ημερομ "
"είναι η αλφαριθμητική ακολουθία που περιέχει την ημ/νια.  Για παράδειγμα, το  "
"DATEVALUE(\"1/1/1999\") ισούται με 36160. \n"
"@ΠΑΡΑΔΕΙΓΜΑΤΑ=\n"
"\n"
"@ΔΕΙΤΕ=DATE"

#: src/functions/fn-date.c:99
msgid ""
"@FUNCTION=EDATE\n"
"@SYNTAX=EDATE(date,months)\n"
"@DESCRIPTION=EDATE returns the serial number of the date that is the "
"specified number of months before or after a given date.  @date is the "
"serial number of the initial date and @months is the number of months before "
"(negative number) or after (positive number) the initial date.\n"
"If @months is not an integer, it is truncated.\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO=DATE"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=EDATE\n"
"@ΣΥΝΤΑΞΗ=EDATE(ημερομ,μήνες)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η EDATE επιστρέφει το σειριακό αριθμό την ημ/νίας που είναι κατά "
"τον καθορισμένο αριθμό μηνών μικρότερη από τη δοθείσα ημ/νία.  Η @ημερομ είναι "
"ο σειριακός αριθμός της αρχικής ημ/νίας και @μήνες είναι ο αριθμός μηνών πριν "
"(αρνητικός αριθμός) ή μετά (θετικός αριθμός) την αρχική ημ/νία.\n"
"Εαν οι @μήνες δεν είναι ακέραιος αριθμός, λαμβάνεται το ακέραιο τμήμα.\n"
"@ΠΑΡΑΔΕΙΓΜΑΤΑ=\n"
"\n"
"@ΔΕΙΤΕ=DATE"

#: src/functions/fn-date.c:147
msgid ""
"@FUNCTION=TODAY\n"
"@SYNTAX=TODAY ()\n"
"@DESCRIPTION=Returns the serial number for today (the number of days elapsed "
"since the 1st of January of 1900).\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO=TODAY, NOW"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=TODAY\n"
"@ΣΥΝΤΑΞΗ=TODAY ()\n"
"@ΠΕΡΙΓΡΑΦΗ=Επιστρέφει το σειριακό αριθμό (πλήθος ημερών από την 1η Ιανουαρίου "
"του 1900) της τρέχουσας ημ/νίας.\n"
"@ΠΑΡΑΔΕΙΓΜΑΤΑ=\n"
"\n"
"@ΔΕΙΤΕ=TODAY, NOW"

#: src/functions/fn-date.c:168
msgid ""
"@FUNCTION=NOW\n"
"@SYNTAX=NOW ()\n"
"@DESCRIPTION=Returns the serial number for the date and time at the time it "
"is evaluated.\n"
"Serial Numbers in Gnumeric are represented as follows:The integral part is "
"the number of days since the 1st of January of 1900.  The decimal part "
"represent the fraction of the day and is mapped into hour, minutes and "
"seconds.\n"
"For example: .0 represents the beginning of the day, and 0.5 represents "
"noon.\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO=TODAY, NOW"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=NOW\n"
"@ΣΥΝΤΑΞΗ=NOW ()\n"
"@ΠΕΡΙΓΡΑΦΗ=Επιστρέφει το σειριακό αριθμό της ημ/νίας και την ώρα κατά τη "
"στγμή της εκτίμησής της.\n"
"Οι σειριακοί αριθμοί αναπαριστώνται στο Gnumeric ως εξής: Το ακέραιο μέρος "
"είναι ο αριθμός των ημερών από την 1η Ιανουαρίου του 1900.  Το δεκαδικό μέρος "
"αντιπροσωπεύει κλάσμα ημέρας και αντιστοιχεί σε ώρα, λεπτά και "
"δευτερόλεπτα.\n"
"Για παράδειγμα: Το .0 αντιστοιχεί στην αρχή της ημέρας και το 0.5 αντιστοιχεί "
"στο μεσημέρι.\n"
"@ΠΑΡΑΔΕΙΓΜΑΤΑ=\n"
"\n"
"@ΔΕΙΤΕ=TODAY, NOW"

#: src/functions/fn-date.c:197
msgid ""
"@FUNCTION=TIME\n"
"@SYNTAX=TIME (hours,minutes,seconds)\n"
"@DESCRIPTION=Returns a fraction representing the time of day.\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO=HOUR"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=TIME\n"
"@ΣΥΝΤΑΞΗ=TIME (ώρες,λεπτά,δευτερόλεπτα)\n"
"@ΠΕΡΙΓΡΑΦΗ=Επιστρέφει το κλάσμα που αντιστοιχεί στην ώρα της ημέρας.\n"
"@ΠΑΡΑΔΕΙΓΜΑΤΑ=\n"
"\n"
"@ΔΕΙΤΕ=HOUR"

#: src/functions/fn-date.c:224
msgid ""
"@FUNCTION=TIMEVALUE\n"
"@SYNTAX=TIMEVALUE (timetext)\n"
"@DESCRIPTION=Returns a fraction representing the time of day, a number "
"between 0 and 1.\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO=HOUR"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=TIMEVALUE\n"
"@ΣΥΝΤΑΞΗ=TIMEVALUE (κειμενο-ώρας)\n"
"@ΠΕΡΙΓΡΑΦΗ=Επιστρέφει το κλάσμα που αντιστοιχεί στην ώρα της ημέρας "
"(αριθμός μεταξύ 0 και 1).\n"
"@ΠΑΡΑΔΕΙΓΜΑΤΑ=\n"
"\n"
"@ΔΕΙΤΕ=HOUR"

#: src/functions/fn-date.c:247
msgid ""
"@FUNCTION=HOUR\n"
"@SYNTAX=HOUR (serial_number)\n"
"@DESCRIPTION=Converts a serial number to an hour.  The hour is returned as "
"an integer in the range 0 (12:00 A.M.) to 23 (11:00 P.M.).\n"
"Note that Gnumeric will perform regular string to serial number conversion "
"for you, so you can enter a date as a string.\n"
"\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO=MINUTE, NOW, TIME, SECOND"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=HOUR\n"
"@ΣΥΝΤΑΞΗ=HOUR (σειριακός_αρ)\n"
"@ΠΕΡΙΓΡΑΦΗ=Μετατρέπει σειριακό αριθμό σε ώρα.  Η ώρα επιστρέφεται ως ακέραιος "
"στο διάστημα 0 (12:00 Π.Μ.) εώς 23 (11:00 Μ.Μ.).\n"
"Το Gnumeric θα κάνει κανονική μετατροπή αλφαριθμητικού σε σειριακό αριθμό "
"για σας, ώστε να μπορείτε να εισάγετε την ημ/νία ως κείμενο.\n"
"\n"
"@ΠΑΡΑΔΕΙΓΜΑΤΑ=\n"
"\n"
"@ΔΕΙΤΕ=MINUTE, NOW, TIME, SECOND"

#: src/functions/fn-date.c:274
msgid ""
"@FUNCTION=MINUTE\n"
"@SYNTAX=MINUTE (serial_number)\n"
"@DESCRIPTION=Converts a serial number to a minute.  The minute is returned "
"as an integer in the range 0 to 59.\n"
"Note that Gnumeric will perform regular string to serial number conversion "
"for you, so you can enter a date as a string.\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO=HOUR, NOW, TIME, SECOND"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=MINUTE\n"
"@ΣΥΝΤΑΞΗ=MINUTE (σειριακός_αρ)\n"
"@ΠΕΡΙΓΡΑΦΗ=Μετατρέπει σειριακό αριθμό σε λεπτά. Τα λεπτά επιστρέφονται ως "
"ακέραιος στο διάστημα 0 εώς 59.\n"
"Το Gnumeric θα κάνει κανονική μετατροπή αλφαριθμητικού σε σειριακό αριθμό "
"για σας, ώστε να μπορείτε να εισάγετε την ημ/νία ως κείμενο.\n"
"\n"
"@ΠΑΡΑΔΕΙΓΜΑΤΑ=\n"
"\n"
"@ΔΕΙΤΕ=HOUR, NOW, TIME, SECOND"

#: src/functions/fn-date.c:302
msgid ""
"@FUNCTION=SECOND\n"
"@SYNTAX=SECOND (serial_number)\n"
"@DESCRIPTION=Converts a serial number to a second.  The second is returned "
"as an integer in the range 0 to 59.\n"
"Note that Gnumeric will perform regular string to serial number conversion "
"for you, so you can enter a date as a string.\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO=HOUR, MINUTE, NOW, TIME"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=SECOND\n"
"@ΣΥΝΤΑΞΗ=SECOND (σειριακός_αρ)\n"
"@ΠΕΡΙΓΡΑΦΗ=Μετατρέπει σειριακό αριθμό σε δευτερόλεπτα. Τα δ/λεπτά επιστρέφονται ως "
"ακέραιος στο διάστημα 0 εώς 59.\n"
"Το Gnumeric θα κάνει κανονική μετατροπή αλφαριθμητικού σε σειριακό αριθμό "
"για σας, ώστε να μπορείτε να εισάγετε την ημ/νία ως κείμενο.\n"
"\n"
"@ΠΑΡΑΔΕΙΓΜΑΤΑ=\n"
"\n"
"@ΔΕΙΤΕ=HOUR, MINUTE, NOW, TIME"

#: src/functions/fn-date.c:330
msgid ""
"@FUNCTION=YEAR\n"
"@SYNTAX=YEAR (serial_number)\n"
"@DESCRIPTION=Converts a serial number to a year.\n"
"Note that Gnumeric will perform regular string to serial number conversion "
"for you, so you can enter a date as a string.\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO=DAY, MONTH, TIME, NOW"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=YEAR\n"
"@ΣΥΝΤΑΞΗ=YEAR (σειριακός_αρ)\n"
"@ΠΕΡΙΓΡΑΦΗ=Μετατρέπει σειριακό αριθμό σε έτος.\n"
"Το Gnumeric θα κάνει κανονική μετατροπή αλφαριθμητικού σε σειριακό αριθμό "
"για σας, ώστε να μπορείτε να εισάγετε την ημ/νία ως κείμενο.\n"
"\n"
"@ΠΑΡΑΔΕΙΓΜΑΤΑ=\n"
"\n"
"@ΔΕΙΤΕ=DAY, MONTH, TIME, NOW"

#: src/functions/fn-date.c:360
msgid ""
"@FUNCTION=MONTH\n"
"@SYNTAX=MONTH (serial_number)\n"
"@DESCRIPTION=Converts a serial number to a month.\n"
"Note that Gnumeric will perform regular string to serial number conversion "
"for you, so you can enter a date as a string.\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO=DAY, TIME, NOW, YEAR"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=MONTH\n"
"@ΣΥΝΤΑΞΗ=MONTH (σειριακός_αρ)\n"
"@ΠΕΡΙΓΡΑΦΗ=Μετατρέπει σειριακό αριθμό σε μήνα.\n"
"Το Gnumeric θα κάνει κανονική μετατροπή αλφαριθμητικού σε σειριακό αριθμό "
"για σας, ώστε να μπορείτε να εισάγετε την ημ/νία ως κείμενο.\n"
"\n"
"@ΠΑΡΑΔΕΙΓΜΑΤΑ=\n"
"\n"
"@ΔΕΙΤΕ=DAY, TIME, NOW, YEAR"

#: src/functions/fn-date.c:390
msgid ""
"@FUNCTION=DAY\n"
"@SYNTAX=DAY (serial_number)\n"
"@DESCRIPTION=Converts a serial number to a day.\n"
"Note that Gnumeric will perform regular string to serial number conversion "
"for you, so you can enter a date as a string.\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO=MONTH, TIME, NOW, YEAR"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DAY\n"
"@ΣΥΝΤΑΞΗ=DAY (σειριακός_αρ)\n"
"@ΠΕΡΙΓΡΑΦΗ=Μετατρέπει σειριακό αριθμό σε ημέρα.\n"
"Το Gnumeric θα κάνει κανονική μετατροπή αλφαριθμητικού σε σειριακό αριθμό "
"για σας, ώστε να μπορείτε να εισάγετε την ημ/νία ως κείμενο.\n"
"\n"
"@ΠΑΡΑΔΕΙΓΜΑΤΑ=\n"
"\n"
"@ΔΕΙΤΕ=MONTH, TIME, NOW, YEAR"

#: src/functions/fn-date.c:420
msgid ""
"@FUNCTION=WEEKDAY\n"
"@SYNTAX=WEEKDAY (serial_number)\n"
"@DESCRIPTION=Converts a serial number to a weekday.  FIXME: explain.\n"
"Note that Gnumeric will perform regular string to serial number conversion "
"for you, so you can enter a date as a string.\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO=MONTH, TIME, NOW, YEAR"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=WEEKDAY\n"
"@ΣΥΝΤΑΞΗ=WEEKDAY (σειριακός_αρ)\n"
"@ΠΕΡΙΓΡΑΦΗ=Μετατρέπει σειριακό αριθμό σε ημέρα εβδομάδας.\n"
"Το Gnumeric θα κάνει κανονική μετατροπή αλφαριθμητικού σε σειριακό αριθμό "
"για σας, ώστε να μπορείτε να εισάγετε την ημ/νία ως κείμενο.\n"
"\n"
"@ΠΑΡΑΔΕΙΓΜΑΤΑ=\n"
"\n"
"@ΔΕΙΤΕ=MONTH, TIME, NOW, YEAR"

#: src/functions/fn-date.c:450
msgid ""
"@FUNCTION=DAYS360 \n"
"@SYNTAX=DAYS360 (date1,date2,method)\n"
"@DESCRIPTION=Returns the number of days from @date1 to @date2 following a "
"360-day calendar in which all months are assumed to have 30 days.\n"
"If @method is true, the European method will be used.  In this case, if the "
"day of the month is 31 it will be considered as 30.\n"
"If @method is false or omitted, the US method will be used.  This is a "
"somewhat complicated industry standard method.\n"
"Note that Gnumeric will perform regular string to serial number conversion "
"for you, so you can enter a date as a string.\n"
"@EXAMPLES=\n"
"\n"
"@SEEALSO=MONTH, TIME, NOW, YEAR"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DAYS360 \n"
"@ΣΥΝΤΑΞΗ=DAYS360 (ημερομ1,ημερομ2,μέθοδος)\n"
"@ΠΕΡΙΓΡΑΦΗ=Επιστρέφει τον αριθμό των ημερών μεταξύ @ημερομ1 και @ημερομ2 ακολουθώντας "
"ημερολόγιο 360 ημερών, όπου όλοι οι μήνες έχουν 30 ημέρες.\n"
"Εάν η @μέθοδος είναι αληθής, θα χρησιμοποιηθεί η ευρωπαϊκή μέθοδος.  Σε αυτή την περίπτωση, "
"εάν η ημέρα του μήνα είναι 31, θα θεωρηθεί ως 30.\n"
"Εάν η @μέθοδος είναι ψευδής ή παραλειφθεί, θα χρησιμοποιηθεί η μέθοδος ΗΠΑ.  Πρόκειται για "
"σχετικά περίπλοκη μέθοδο που χρησιμοποιείται τυπικά στις επιχειρήσεις.\n"
"Το Gnumeric θα κάνει κανονική μετατροπή αλφαριθμητικού σε σειριακό αριθμό "
"για σας, ώστε να μπορείτε να εισάγετε την ημ/νία ως κείμενο.\n"
"@ΠΑΡΑΔΕΙΓΜΑΤΑ=\n"
"\n"
"@ΔΕΙΤΕ=MONTH, TIME, NOW, YEAR"

