#: src/functions/fn-database.c:504
msgid ""
"@FUNCTION=DAVERAGE\n"
"@SYNTAX=DAVERAGE(database,field,criteria)\n"
"@DESCRIPTION=DAVERAGE function returns the average of the values in a list "
"or database that match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DAVERAGE\n"
"@ΣΥΝΤΑΞΗ=DAVERAGE(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DAVERAGE επιστρέφει το μέσο όρο των τιμών ενός "
"καταλόγου ή βάσης δεδομένων, η οποίες πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:512
msgid ""
"\n"
"@EXAMPLES=\n"
"DAVERAGE(A1:C7, \"Salary\", A9:A11) equals 42296.3333.\n"
"DAVERAGE(A1:C7, \"Age\", A9:A11) equals 39.\n"
"DAVERAGE(A1:C7, \"Salary\", A9:B11) equals 40782.5.\n"
"DAVERAGE(A1:C7, \"Age\", A9:B11) equals 36.\n"
"\n"
"@SEEALSO=DCOUNT"
msgstr ""

#: src/functions/fn-database.c:574
msgid ""
"@FUNCTION=DCOUNT\n"
"@SYNTAX=DCOUNT(database,field,criteria)\n"
"@DESCRIPTION=DCOUNT function counts the cells that contain numbers in a "
"database that match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DCOUNT\n"
"@ΣΥΝΤΑΞΗ=DCOUNT(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DCOUNT επιστρέφει το πλήθος των κελιών που περιέχουν "
"αριθμούς σε μία βάση δεδομένων και πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:582
msgid ""
"\n"
"@EXAMPLES=\n"
"DCOUNT(A1:C7, \"Salary\", A9:A11) equals 3.\n"
"DCOUNT(A1:C7, \"Salary\", A9:B11) equals 2.\n"
"DCOUNT(A1:C7, \"Name\", A9:B11) equals 0.\n"
"\n"
"@SEEALSO=DAVERAGE"
msgstr ""

#: src/functions/fn-database.c:636
msgid ""
"@FUNCTION=DCOUNTA\n"
"@SYNTAX=DCOUNTA(database,field,criteria)\n"
"@DESCRIPTION=DCOUNTA function counts the cells that contain data in a "
"database that match conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DCOUNTA\n"
"@ΣΥΝΤΑΞΗ=DCOUNTA(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DCOUNTA επιστρέφει το πλήθος των κελιών που περιέχουν "
"δεδομένα σε μία βάση δεδομένων και πληρούν τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:644
msgid ""
"\n"
"@EXAMPLES=\n"
"DCOUNTA(A1:C7, \"Salary\", A9:A11) equals 3.\n"
"DCOUNTA(A1:C7, \"Salary\", A9:B11) equals 2.\n"
"DCOUNTA(A1:C7, \"Name\", A9:B11) equals 2.\n"
"\n"
"@SEEALSO=DCOUNT"
msgstr ""

#: src/functions/fn-database.c:695
msgid ""
"@FUNCTION=DGET\n"
"@SYNTAX=DGET(database,field,criteria)\n"
"@DESCRIPTION=DGET function returns a single value from a column that match "
"conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DGET\n"
"@ΣΥΝΤΑΞΗ=DGET(βάση-δεδομένων,πεδίο,κριτήρια)\n"
"@ΠΕΡΙΓΡΑΦΗ=Η συνάρτηση DGET επιστρέφει μία μόνο τιμή από μια στήλη που "
"πληρεί τις καθορισμένες συνθήκες. \n"

#: src/functions/fn-database.c:703
msgid ""
"\n"
"If none of the items match the conditions, DGET returns #VALUE! error. If "
"more than one items match the conditions, DGET returns #NUM! error. \n"
"@EXAMPLES=\n"
"DGET(A1:C7, \"Salary\", A9:A10) equals 34323.\n"
"DGET(A1:C7, \"Name\", A9:A10) equals \"Clark\".\n"
"\n"
"@SEEALSO=DCOUNT"
msgstr ""

#: src/functions/fn-database.c:764
msgid ""
"@FUNCTION=DMAX\n"
"@SYNTAX=DMAX(database,field,criteria)\n"
"@DESCRIPTION=DMAX function returns the largest number in a column that match "
"conditions specified. \n"
msgstr ""
"@ΣΥΝΑΡΤΗΣΗ=DMAX\n"
"@ΣΥΝΤΑΞΗ=DMAX(βάση-δεδομένων,πεδίο,κριτήρια)\n"
