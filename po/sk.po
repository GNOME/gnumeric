#: src/fn-string.c:139
msgid ""
"@FUNCTION=LOWER\n"
"@SYNTAX=LOWER(text)\n"
"@DESCRIPTION=Returns a lower-case version of the string in @text\n"
"@SEEALSO=UPPER"
msgstr ""

#: src/fn-string.c:172
msgid ""
"@FUNCTION=MID\n"
"@SYNTAX=MID(string, position, length)\n"
