msgid ""
"@FUNCTION=LEFT\n"
"@SYNTAX=LEFT(text[,num_chars])\n"
"@DESCRIPTION=Returns the leftmost num_chars characters or the left character if num_chars is not specified\n"
"@SEEALSO=MID, RIGHT"
