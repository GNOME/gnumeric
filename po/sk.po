#: src/functions/fn-information.c:454
msgid ""
"@FUNCTION=ISERROR\n"
"@SYNTAX=ISERROR(value)\n"
"@DESCRIPTION=ISERROR returns a TRUE value if the expression has an error.\n"
"This function is Excel compatible. \n"
"@EXAMPLES=\n"
"ISERROR(NA()) equals TRUE.\n"
"\n"
"@SEEALSO=ERROR"
msgstr ""
"@FUNCTION=ISERROR\n"
"@SYNTAX=ISERROR(hodnota)\n"
"@DESCRIPTION=ISERROR vracia TRUE ak je výraz chybný\n"
"Táto funkcia je kompatibilná s Excelom. \n"
"@EXAMPLES=\n"
"ISERROR(NA()) sa rovná TRUE.\n"
"\n"
"@SEEALSO=ERROR"

#: src/functions/fn-information.c:478
msgid "Argument mismatch"
msgstr "Chyba argumentu"

#: src/functions/fn-information.c:509
msgid ""
"@FUNCTION=ISNA\n"
"@SYNTAX=ISNA(value)\n"
"@DESCRIPTION=ISNA returns TRUE if the value is the #N/A error value.\n"
"This function is Excel compatible. \n"
"@EXAMPLES=\n"
"ISNA(NA()) equals TRUE.\n"
"\n"
"@SEEALSO=NA"
msgstr ""
"@FUNCTION=ISNA\n"
"@SYNTAX=ISNA(hodnota)\n"
"@DESCRIPTION=ISNA vracia TRUE ak je hodnotou chyba #N/A.\n"
"Táto funkcia je kompatibilná s Excelom. \n"
"@EXAMPLES=\n"
"ISNA(NA()) sa rovná TRUE.\n"
"\n"
"@SEEALSO="

#: src/functions/fn-information.c:544
msgid ""
"@FUNCTION=ISERR\n"
"@SYNTAX=ISERR(value)\n"
"@DESCRIPTION=ISERR returns TRUE if the value is any error value except #N/"
"A.\n"
"This function is Excel compatible. \n"
"@EXAMPLES=\n"
"ISERR(NA()) return FALSE.\n"
"\n"
"@SEEALSO=ISERROR"
msgstr ""
"@FUNCTION=ISERR\n"
"@SYNTAX=ISERR(hodnota)\n"
"@DESCRIPTION=ISERR vracia TRUE ak je hodnotou chyba okrem #N/A.\n"
"Táto funkcia je kompatibilná s Excelom. \n"
"@EXAMPLES=\n"
"ISERR(NA()) sa rovná FALSE.\n"
"\n"
"@SEEALSO="

#: src/functions/fn-information.c:575
msgid ""
"@FUNCTION=ERROR.TYPE\n"
"@SYNTAX=ERROR(value)\n"
"@DESCRIPTION=ERROR.TYPE returns an error number corresponding to the given "
"error value.  The error numbers for error values are\n"
"#DIV/0!    2\n"
"#VALUE!    3\n"
"#REF!      4\n"
"#NAME?     5\n"
"#NUM!      6\n"
"#N/A       7\n"
"This function is Excel compatible. \n"
"@EXAMPLES=\n"
"ERROR.TYPE(NA()) equals 7.\n"
"\n"
"@SEEALSO=ISERROR"
msgstr ""
"@FUNCTION=ERROR.TYPE\n"
"@SYNTAX=ERROR(hodnota)\n"
"@DESCRIPTION=ERROR.TYPE vracia číslo chyby podľa danej chybovej hodnoty. "
"Čísla chybových hodnôt sú\n"
"#DIV/0!    2\n"
"#VALUE!    3\n"
"#REF!      4\n"
"#NAME?     5\n"
"#NUM!      6\n"
"#N/A       7\n"
"Táto funkcia je kompatibilná s Excelom. \n"
"@EXAMPLES=\n"
"ERROR.TYPE(NA()) sa rovná 7.\n"
"\n"
"@SEEALSO=ISERROR"

#: src/functions/fn-information.c:634
msgid ""
"@FUNCTION=NA\n"
"@SYNTAX=NA()\n"
"@DESCRIPTION=NA returns the error value #N/A.\n"
"This function is Excel compatible. \n"
"@EXAMPLES=\n"
"NA() equals #N/A error.\n"
"\n"
"@SEEALSO=ISNA"
msgstr ""
"@FUNCTION=NA\n"
"@SYNTAX=NA()\n"
"@DESCRIPTION=NA vracia chybovú hodnotu #N/A.\n"
"Táto funkcia je kompatibilná s Excelom. \n"
"@EXAMPLES=\n"
"NA() sa rovná chybe #N/A.\n"
"\n"
"@SEEALSO="

#: src/functions/fn-information.c:656
msgid ""
"@FUNCTION=ERROR\n"
"@SYNTAX=ERROR(text)\n"
"@DESCRIPTION=ERROR return the specified error.\n"
"\n"
"@EXAMPLES=\n"
"ERROR(\"#OWN ERROR\").\n"
"\n"
"@SEEALSO=ISERROR"
msgstr ""
"@FUNCTION=ERROR\n"
"@SYNTAX=ERROR(text)\n"
"@DESCRIPTION=ERROR vracia uvedenú chybu\n"
"\n"
"@EXAMPLES=\n"
"ERROR(\"#MOJA CHYBA\").\n"
"\\n@SEEALSO=ISERROR"

#: src/functions/fn-information.c:677
msgid ""
"@FUNCTION=ISBLANK\n"
"@SYNTAX=ISBLANK(value)\n"
"@DESCRIPTION=ISBLANK returns TRUE if the value is blank.\n"
"This function is Excel compatible. \n"
"@EXAMPLES=\n"
"ISBLANK(A1).\n"
"\n"
"@SEEALSO="
msgstr ""
"@FUNCTION=ISBLANK\n"
"@SYNTAX=ISBLANK(hodnota)\n"
"@DESCRIPTION=ISBLANK vracia TRUE ak je hodnota prázdna.\n"
"Táto funkcia je kompatibilná s Excelom. \n"
"@EXAMPLES=\n"
"ISBLANK(A1).\n"
"\n"
"@SEEALSO="

#: src/functions/fn-information.c:726
msgid ""
"@FUNCTION=ISEVEN\n"
"@SYNTAX=ISEVEN(value)\n"
"@DESCRIPTION=ISEVEN returns TRUE if the number is even.\n"
"This function is Excel compatible. \n"
"@EXAMPLES=\n"
"ISEVEN(4) equals TRUE.\n"
"\n"
"@SEEALSO=ISODD"
msgstr ""
"@FUNCTION=ISEVEN\n"
"@SYNTAX=ISEVEN(hodnota)\n"
"@DESCRIPTION=ISEVEN vracia TRUE ak je číslo párne.\n"
