#~ "\n"
#~ "@SEEALSO=WORKDAY"
#~ msgstr ""
#~ "@FUNCTION=NETWORKDAYS\n"
#~ "@SYNTAX=NETWORKDAYS (data_inici,data_final,festius)\n"
#~ "@DESCRIPTION=Retorna el nombre de dies que no siguin caps de setmana ni "
