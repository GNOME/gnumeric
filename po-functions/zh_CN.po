#~ "\n"
#~ "@SEEALSO=LINEST"
#~ msgstr ""
#~ "@FUNCTION=TREND\n"
#~ "@SYNTAX=TREND(known_y's[,known_x's],new_x's])\n"
#~ "@DESCRIPTION=TREND  ``''"
#~ ". @known_y's  y-,  y=mx+b,  @known_x  "
#~ "x-. @new_x's  y- x-. \n"
#~ " @known_x's ,  {1, 2, 3, ...} .  @new_x's , "
#~ " @known_x's .  @known_y's  @known_x's "
#~ ",  TREND  #NUM! . \n"
#~ "@EXAMPLES=\n"
#~ " A1, A2, ..., A5  11.4, 17.3, 21.3, 25.9,  40.1, "
#~ " B1, B2, ... B5  23.2, 25.8, 29.9, 33.5,  42.7.  \n"
#~ "TREND(A1:A5,B1:B5) = {12.1, 15.7, 21.6, 26.7, 39.7}.\n"
#~ "\n"
#~ "@SEEALSO=LINEST"

