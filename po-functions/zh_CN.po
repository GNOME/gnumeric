#~ "1 52  53 ISOWEEKNUM  ISOYEAR "
#~ "\n"
#~ "\n"
#~ "*  date ISOWEEKNUM  #NUM!\n"
#~ "\n"
#~ "@EXAMPLES=\n"
#~ " A1  12/21/00 ISOWEEKNUM(A1)=51\n"
#~ "@SEEALSO=WEEKNUM, ISOYEAR"

