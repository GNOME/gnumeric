#~ "1  /\n"
#~ "2  /360\n"
#~ "3  /365\n"
#~ "4  30/360()\n"
#~ "\n"
#~ "@basis, 30/360().  @settlement "
#~ " @maturity ,  INTRATE  #NUM! .  @basis < "
#~ "0  @basis > 4,  INTRATE  #NUM! .  @settlement  "
#~ "@maturity ,  @maturity,  INTRATE  #NUM! "
#~ ". \n"
#~ "@EXAMPLES=\n"
#~ "\n"
#~ "2000415, 2000930,  "
#~ "$100,000,  $103,525, /, "
