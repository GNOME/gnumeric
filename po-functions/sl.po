msgstr "wiki:en:Hyperbolic_function"

#: ../plugins/fn-math/functions.c:945
msgid "DEGREES:equivalent degrees to @{x} radians"
msgstr "DEGREES: stopinje v @{x} radianih"

#: ../plugins/fn-math/functions.c:963
msgid "EXP:e raised to the power of @{x}"
msgstr "EXP: število e na potenco vrednosti @{x}"

#: ../plugins/fn-math/functions.c:965
msgid "e is the base of the natural logarithm."
msgstr "Vrednost e je osnova naravnega logaritma."

#: ../plugins/fn-math/functions.c:988
msgid "EXPM1:EXP(@{x})-1"
msgstr "EXPM1:EXP(@{x})-1"

#: ../plugins/fn-math/functions.c:990
