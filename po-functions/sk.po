#~ "@EXAMPLES=\n"
#~ "\n"
#~ "@SEEALSO=PPMT,PV,FV,DURATION,MDURATION"
#~ msgstr ""
#~ "@FUNCTION=DURATION\n"
#~ "@SYNTAX=DURATION(úrok,pv,fv)\n"
#~ "@DESCRIPTION=DURATION počíta počet období, za ktoré investícia získa danú "
#~ "hodnotu. Táto funkcia je podobná FV a PV s tým rozdielom, že nie je "
#~ "potrebné určovať smer peňažného toku, napr. -100 pre odtok a +100 pre "
#~ "prítok.\n"
#~ "@EXAMPLES=\n"
#~ "\n"
#~ "@SEEALSO=PPMT,PV,FV"

