msgstr "name:cadena"

#: ../plugins/fn-info/functions.c:1607
msgid "ISBLANK:TRUE if @{value} is blank"
msgstr "ISBLANK:VERDADERO si @{valor} está vacío"

#: ../plugins/fn-info/functions.c:1609
