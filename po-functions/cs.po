"\n"
"\t#DĚLENÍ/0!\t2\n"
"\t#HODNOTA!\t3\n"
"\t#ODKAZ!  \t4\n"
"\t#NÁZEV?  \t5\n"
"\t#ČÍSLO!  \t6\n"
"\t#N/A     \t\t7"

#: ../plugins/fn-info/functions.c:1572
msgid "NA:the error value #N/A"
msgstr "NA:chybová hodnota #N/A"

