msgstr "ISODD : TRUE(VRAI) si @{n} est impair"

#: ../plugins/fn-info/functions.c:1680
msgid "ISREF:TRUE if @{value} is a reference"
msgstr "ISREF : TRUE(VRAI) si @{valeur} est une référence"

#: ../plugins/fn-info/functions.c:1682
