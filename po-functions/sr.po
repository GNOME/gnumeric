msgid "LOG2:the base-2 logarithm of @{x}"
msgstr "LOG2:логаритам @{х}-а основе 2"

#: ../plugins/fn-math/functions.c:1496
msgid "If @{x} ≤ 0, LOG2 returns #NUM!"
msgstr "Ако је @{x} ≤ 0, „LOG2“ исписује #БРОЈ!"

