#~ "@EXAMPLES=\n"
#~ "\n"
#~ "@SEEALSO=PV,PMT,PPMT"

#~ msgid ""
#~ "@FUNCTION=PMT\n"
#~ "@SYNTAX=PMT(rate,nper,pv[,fv,type])\n"
