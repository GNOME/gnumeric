#~ "@EXAMPLES=\n"
#~ "\n"
#~ "@SEEALSO=FV"

#~ msgid ""
#~ "@FUNCTION=NPV\n"
#~ "@SYNTAX=NPV(rate,v1,v2,...)\n"
