"\t#DIV/0!  \t\t2\n"
"\t#VALOR!  \t3\n"
"\t#REF!    \t\t4\n"
"\t#NOME?   \t5\n"
"\t#NUM!    \t6\n"
"\t#N/D     \t\t7"

