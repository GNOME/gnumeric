#~ "@volatilidade1 & @volatilidade2 são as taxas anualizadas de volatilidade "
#~ "dos ativos envolvidos.\n"
#~ "\n"
#~ "@EXAMPLES=\n"
#~ "\n"
#~ "@SEEALSO=OPT_BS, OPT_BS_DELTA, OPT_BS_RHO, OPT_BS_THETA, OPT_BS_GAMMA"

